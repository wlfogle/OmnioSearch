{
  "name": "omniosearch",
  "private": true,
  "version": "1.0.0",
  "description": "👁️ OmnioSearch - The All-Seeing File Search Engine (Native Desktop App)",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
