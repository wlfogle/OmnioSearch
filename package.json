    "@tauri-apps/plugin-shell": "^2.3.0",
    "@tauri-apps/plugin-window-state": "^2.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "@headlessui/react": "^2.1.2",
    "@heroicons/react": "^2.1.5",
    "clsx": "^2.1.1",
    "framer-motion": "^11.3.28",
    "react-hot-toast": "^2.4.1",
